# change window focus within space
ctrl + shift - j : yabai -m window --focus south
ctrl + shift - k : yabai -m window --focus north
ctrl + shift - h : yabai -m window --focus west
ctrl + shift - l : yabai -m window --focus east

# swap window
alt + shift - j : yabai -m window --swap south
alt + shift - k : yabai -m window --swap north
alt + shift - h : yabai -m window --swap west
alt + shift - l : yabai -m window --swap east

# resize width window
cmd + ctrl + alt - j : yabai -m window --resize bottom:0:50; yabai -m window --resize top:0:50
cmd + ctrl + alt - k : yabai -m window --resize top:0:-50; yabai -m window --resize bottom:0:-50
cmd + ctrl + alt - h : yabai -m window --resize right:-50:0; yabai -m window --resize left:-50:0
cmd + ctrl + alt - l : yabai -m window --resize right:50:0; yabai -m window --resize left:50:0

# maximize focus window
cmd + ctrl + alt - m : yabai -m window --toggle zoom-fullscreen

#minimize focus window
cmd + ctrl + alt - i : yabai -m window --minimize

# move window to prev and next space
cmd + ctrl + alt - p : yabai -m window --space prev;
cmd + ctrl + alt - n : yabai -m window --space next;